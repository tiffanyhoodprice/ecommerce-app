<%= form_tag "/suppliers/#{@supplier.id}/edit", method: :patch do %>
  <div>
<%= label_tag :user %>
<%= text_field_tag :user, current_user.email %>
  </div>

   <div>
<%= label_tag :name %>
<%= text_field_tag :name, @supplier.name %>
  </div>

<div>
<%= label_tag :email %>
<%= text_area_tag :email, @supplier.email %>
  </div> 

<div>
<%= label_tag :phone %>
<%= text_area_tag :phone, @supplier.phone %>
  </div> 

<div>
<%= submit_tag "Update Supplier Info" %>
</div>

<% end %> 