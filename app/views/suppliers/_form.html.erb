<%= form_for @supplier do |f| %> 
  <div>
    <%= f.label :company_name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
 
  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.label :email_address %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>